{
  "name": "scout-ua",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "npm run lint && npm run unit-test && npm run integ-test",
    "unit-test":
      "nyc --reporter=text mocha test/unit && nyc report --reporter=html",
    "travis-unit":
      "nyc mocha test/unit && nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "integ-test": "mocha ./test/integration"
  },
  "dependencies": {
    "audioconcat": "^0.1.3",
    "aws-sdk": "^2.224.1",
    "babel-eslint": "^8.2.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.18.2",
    "chai": "^4.1.2",
    "chunk-text": "^1.0.5",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "dynamoose": "^0.8.7",
    "eslint": "^4.18.2",
    "express": "~4.15.5",
    "get-website-favicon": "0.0.7",
    "glob": "^7.1.2",
    "html-metadata": "^1.7.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.2.0",
    "morgan": "~1.9.0",
    "natural": "^0.6.1",
    "node-uuid-generator": "^1.0.0",
    "nyc": "^11.6.0",
    "readability-js": "^1.0.7",
    "request-promise": "^4.2.2",
    "serve-favicon": "~2.4.5",
    "universal-analytics": "^0.4.17",
    "url": "^0.11.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.2",
    "eslint-config-google": "^0.9.1",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha": "^5.2.0",
    "nock": "^9.4.1",
    "rewire": "^4.0.1",
    "sinon": "^6.1.3"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "moduleFileExtensions": ["js", "json"],
    "testMatch": ["**/test/**/*-test.(js)"],
    "mapCoverage": true,
    "setupTestFrameworkScriptFile": "<rootDir>/test/common.js",
    "testEnvironment": "node"
  }
}
